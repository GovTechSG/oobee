<script>
/**
 * Decodes a Base64-encoded, GZIP-compressed JSON string.
 * @param {string} base64String - The encoded string
 * @returns {Promise<Object>} - The parsed JSON object
 */
async function decodeUnzipParse(base64String) {
  try {
    // Step 1: Base64 decode to Uint8Array
    const binaryString = atob(base64String);
    const bytes = new Uint8Array(binaryString.length);
    for (let i = 0; i < binaryString.length; i++) {
      bytes[i] = binaryString.charCodeAt(i);
    }

    // Step 2: Decompress with fflate (GZIP)
    const decompressed = new TextDecoder().decode(fflate.gunzipSync(bytes));

    // Step 3: Parse JSON
    const json = JSON.parse(decompressed);

    return json;
  } catch (err) {
    throw new Error(`Failed to decode/unzip/parse: ${err.message}`);
  }
}
</script>