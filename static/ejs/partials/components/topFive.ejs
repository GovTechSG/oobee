<div id="top-five-card" class="card h-100">
  <div class="card-body">
    <h2>Top 5 affected pages with the most issues</h2>
    <% topFiveMostIssues.forEach((page, index) => { %>
      <div class="d-flex justify-content-between">
        <a href="<%= page.url %>" target="_blank"><%= page.pageTitle %></a>
        <span aria-label="<%= page.totalIssues %> must fix items" class="fw-bold ms-2"><%= page.totalIssues %></span>
      </div>
      <% if (index < topFiveMostIssues.length - 1) { %>
        <hr class="my-2"/>
      <% } %> 
    <% }) %>
  </div>
</div>
